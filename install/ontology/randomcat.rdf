<!-- 
The Ontology definition of the Ekstera's Sliced Test Model. This Test Model aims
at demonstrating the ability of TAO to run Computer Adaptive Tests (CAT).
-->
<rdf:RDF 
    xml:base="http://www.tao.lu/Ontologies/TAOTest.rdf#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:test="http://www.tao.lu/Ontologies/TAOTest.rdf#">
    <rdf:Description rdf:about="http://www.tao.lu/Ontologies/TAOTest.rdf#RandomCATTestModel">
        <rdf:type rdf:resource="http://www.tao.lu/Ontologies/TAOTest.rdf#TestModel"/>
        <rdfs:label xml:lang="en-US">Random CAT</rdfs:label>
        <rdfs:comment xml:lang="en-US">A basic implementation of TAO CAT using random item selection.</rdfs:comment>
        <test:TestModelImplementation>oat\taoRandomCat\model\RandomModel</test:TestModelImplementation>
    </rdf:Description>
</rdf:RDF>